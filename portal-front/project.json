{
  "name": "portal-front",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "portal-front",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project portal-front --web",
  "targets": {
    "dev-prisma": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["prisma generate --watch", "nx dev portal-front"]
      }
    },
    "build": {
      "dependsOn": ["prisma"]
    },
    "prisma": {
      "executor": "nx:run-commands",
        "options": {
            "command": "prisma generate"
        }
    }
  }
}
